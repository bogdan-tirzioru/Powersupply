#include "processing.h"
#define ADCVoltageReferince ((double)3.0)
#define ADCNrBitRezolution 12
double Convert_to_Voltage(uint16_t ADC_LiniarVoltage)
{
	double ret_Value=0;
	ret_Value = ADC_LiniarVoltage * (((uint32_t)1)<<ADCNrBitRezolution)/ADCVoltageReferince;
	return ret_Value;
}
double ADC_to_Physical_VoltageDivider(uint16_t d_VoltageADC,uint32_t ui32RezistorUper,uint32_t ui32RezistorLower)
{
	double ret_Value=0;
	return ret_Value;
}
