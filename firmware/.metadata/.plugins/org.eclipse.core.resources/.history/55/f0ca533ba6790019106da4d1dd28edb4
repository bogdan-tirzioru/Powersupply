#include "micro.h"
#include "signal_proc.h"

/*-----------------------------------------------------------------------------------*/
/*Description:  Function to convert raw values to voltage in physical                   */
/*                                                                                   */
/*Parameters:   uw_voltage (input) voltage input in Q20.12 format                    */
/*Return value: voltage in double                                             */
/*-----------------------------------------------------------------------------------*/
double Convert_to_Voltage (T_ULONG ul_raw_value)
{
 double ld_result = 0;
 ld_result = ((double)ul_raw_value) * Voltage_prez_rez;
 return ld_result;
}


double ADC_to_Physical_VoltageDivider(double dAdcVoltage,uint16_t lui16_UpperRezistor,uint16_t lui16_LowerRezistor)
{
	double ret_dVoltage=0;
	ret_dVoltage = (dAdcVoltage * lui16_UpperRezistor)/(lui16_UpperRezistor+lui16_LowerRezistor);
	return ret_dVoltage;
}


